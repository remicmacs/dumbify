{
  "manifest_version": 2,
  "name": "Dumbify",
  "version": "0.0.1",

  "description": "DuMBiFy TeXT iN alL paGeS",

  "icons": {
    "48": "icons/border-48.png"
  },

  "browser_action": {
    "default_icon": "icons/dumb.svg",
    "default_title": "Dumbify",
    "default_popup": "popup/popup.html"
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["dumbify.js"]
  }]
}
